{
    "version": 2,
    "builds": [
        {
            "src": "flask_api.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9",
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/flask_api.py"
        },
        {
            "src": "/(.*)",
            "dest": "/flask_api.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "FLASK_ENV": "production"
    },
    "functions": {
        "flask_api.py": {
            "maxDuration": 30,
            "memory": 1024
        }
    },
    "regions": ["iad1"]
}